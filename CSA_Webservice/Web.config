        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="MyAspNetAjaxBehavior">
          <!--<enableWebScript/>-->
          <webHttp defaultOutgoingResponseFormat="Json"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true"/>
    <bindings>
      <wsHttpBinding>
        <binding name="MyWSHttpBinding" closeTimeout="00:02:00" sendTimeout="00:05:00" bypassProxyOnLocal="true" maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="2000000" maxStringContentLength="2147483647" maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647"/>
        </binding>
      </wsHttpBinding>
